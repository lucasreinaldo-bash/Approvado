<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:orientation="vertical">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginEnd="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="309dp">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="224dp"
                android:layout_height="107dp"
                android:layout_gravity="center"
                app:srcCompat="@drawable/logoconcurso"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="0dp" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/aclonica"
                android:padding="15sp"
                android:text="Matemática"
                android:textAlignment="center"
                android:textStyle="bold" />

            <CheckBox
                android:id="@+id/checkBox1"
                android:padding="10sp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Números Reais"

 />

            <CheckBox
                android:id="@+id/checkBox2"
                android:padding="10sp"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:text="Números Complexos" />

            <CheckBox
                android:id="@+id/checkBox3"
                android:padding="10sp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Sistemas Lineares" />

            <CheckBox
                android:id="@+id/checkBox4"
                android:padding="10sp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Matrizes e Determinantes"
                />

            <CheckBox
                android:id="@+id/checkBox5"
                android:padding="10sp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Progressão aritmética e Progressão geométrica"
/>

            <CheckBox
                android:id="@+id/checkBox6"
                android:padding="10sp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Polinômios" />

            <CheckBox
                android:id="@+id/checkBox7"
                android:padding="10sp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Equações e Inequações" />

            <CheckBox
                android:id="@+id/checkBox8"
                android:padding="10sp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Logarítmicas e Modulares"
/>

            <CheckBox
                android:id="@+id/checkBox9"
                android:padding="10sp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Exponenciais" />

            <CheckBox
                android:id="@+id/checkBox10"
                android:padding="10sp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Trigonometria" />
            <CheckBox
                android:id="@+id/checkBox11"
                android:padding="10sp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Função Modular" />
            <CheckBox
                android:id="@+id/checkBox12"
                android:padding="10sp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Geomatria Plana" />
            <CheckBox
                android:id="@+id/checkBox13"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Geometria Espacial" />
            <CheckBox
                android:id="@+id/checkBox14"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Analise Combinatória" />
            <CheckBox
                android:id="@+id/checkBox15"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Estudo das probabilidades" />
            <CheckBox
                android:id="@+id/checkBox16"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Estatística" />
            <CheckBox
                android:id="@+id/checkBox17"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Matemática Financeira" />



            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/logoconcurso"
                />

        </LinearLayout>


    </ScrollView>



</android.support.constraint.ConstraintLayout>